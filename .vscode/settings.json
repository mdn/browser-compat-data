{
  "sortJSON.orderOverride": [
    "version_added",
    "version_removed"
  ],
  "sortJSON.orderUnderride": [
    "status",
    "standard_track",
    "deprecated"
  ],
  "json.schemas": [
    {
      "fileMatch": [
        "/api/*.json",
        "/css/*.json",
        "/html/*.json",
        "/http/*.json",
        "/javascript/*.json",
        "/mathml/*.json",
        "/svg/*.json",
        "/webdriver/*.json",
        "/webextensions/*.json"
      ],
      "url": "/schemas/compat-data.schema.json"
    },
    {
      "fileMatch": [
        "/browsers/*.json"
      ],
      "url": "/schemas/browsers.schema.json"
    }
  ]
}
