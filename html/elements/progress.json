                "version_added": "11"
              },
              "opera_android": {
                "version_added": "11"
              },
              "safari": {
                "version_added": "6"
              },
              "safari_ios": {
                "version_added": "7"
              },
              "samsunginternet_android": "mirror",
              "webview_android": "mirror",
              "webview_ios": "mirror"
            },
            "status": {
              "experimental": false,
              "standard_track": true,
              "deprecated": false
            }
          }
        },
        "value": {
          "__compat": {
