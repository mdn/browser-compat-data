{
  "html": {
    "elements": {
      "strong": {
        "__compat": {
          "mdn_url": "https://developer.mozilla.org/docs/Web/HTML/Reference/Elements/strong",
          "spec_url": "https://html.spec.whatwg.org/multipage/text-level-semantics.html#the-strong-element",
          "tags": [
            "web-features:strong"
          ],
          "support": {
            "chrome": {
              "version_added": "1"
            },
            "chrome_android": "mirror",
            "edge": {
              "version_added": "12"
            },
            "firefox": {
              "version_added": "1",
              "notes": "Before Firefox 4, creating a `<strong>` element incorrectly resulted in an `HTMLSpanElement` object, instead of the expected `HTMLElement`."
            },
            "firefox_android": {
              "version_added": "4"
            },
            "ie": {
              "version_added": "≤11"
            },
            "oculus": "mirror",
            "opera": "mirror",
            "opera_android": "mirror",
            "safari": {
              "version_added": "≤4"
            },
            "safari_ios": "mirror",
            "samsunginternet_android": "mirror",
            "webview_android": "mirror",
            "webview_ios": "mirror"
          },
          "status": {
            "experimental": false,
            "standard_track": true,
            "deprecated": false
          }
        }
      }
    }
  }
}
