              "experimental": false,
              "standard_track": true,
              "deprecated": false
            }
          }
        },
        "fixed": {
          "__compat": {
            "spec_url": "https://drafts.csswg.org/css-position/#valdef-position-fixed",
            "tags": [
              "web-features:fixed-positioning"
            ],
            "support": {
              "chrome": {
                "version_added": "1"
              },
              "chrome_android": "mirror",
              "edge": {
                "version_added": "12"
              },
              "firefox": {
                "version_added": "1",
                "notes": "Before Firefox 44, `position: fixed` didn't create a stacking context in most cases. Firefox and the specification have been modified to mimic Chrome and Safari's long-time behavior."
              },
              "firefox_android": "mirror",
              "ie": {
                "version_added": "7",
                "notes": "In Internet Explorer, fixed positioning doesn't work if the document is in [quirks mode](https://developer.mozilla.org/docs/Web/HTML/Quirks_Mode_and_Standards_Mode)."
              },
              "oculus": "mirror",
              "opera": {
                "version_added": "4"
              },
              "opera_android": "mirror",
              "safari": {
                "version_added": "1"
              },
              "safari_ios": "mirror",
              "samsunginternet_android": "mirror",
              "webview_android": "mirror",
              "webview_ios": "mirror"
            },
            "status": {
              "experimental": false,
              "standard_track": true,
              "deprecated": false
            }
          }
        },
        "position_sticky_table_elements": {
          "__compat": {
            "description": "Table elements as `sticky` positioning containers",
            "tags": [
              "web-features:sticky-positioning"
