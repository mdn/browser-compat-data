            "support": {
              "chrome": {
                "version_added": "56"
              },
              "chrome_android": "mirror",
              "edge": {
                "version_added": "16"
              },
              "firefox": {
                "version_added": "59",
                "notes": "Borders do not display on table headers if [`border-collapse`](https://developer.mozilla.org/docs/Web/CSS/border-collapse) is set to `collapse` ([bug 1727594](https://bugzil.la/1727594))."
              },
              "firefox_android": "mirror",
              "ie": {
