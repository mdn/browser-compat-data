              {
                "prefix": "-webkit-",
                "version_added": "12"
              }
            ],
            "firefox": [
              {
                "version_added": "4",
                "notes": "Before Firefox 50, border styles of rounded corners were always rendered as if <code>border-style</code> was solid. This has been fixed in Firefox 50."
              },
              {
                "prefix": "-webkit-",
                "version_added": "49"
              },
              {
                "alternative_name": "-moz-border-radius-topleft",
                "version_added": "1",
                "version_removed": "12"
              }
