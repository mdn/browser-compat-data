              {
                "version_added": "4.2"
              },
              {
                "prefix": "-webkit-",
                "version_added": "1"
              }
            ],
            "samsunginternet_android": "mirror",
            "webview_android": [
              {
                "version_added": "≤37"
              },
              {
                "prefix": "-webkit-",
                "version_added": "≤37"
              }
            ]
          },
