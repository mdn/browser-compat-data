{
  "css": {
    "properties": {
      "border-top-style": {
        "__compat": {
          "mdn_url": "https://developer.mozilla.org/docs/Web/CSS/border-top-style",
          "spec_url": "https://drafts.csswg.org/css-backgrounds/#the-border-style",
          "tags": [
            "web-features:borders"
          ],
          "support": {
            "chrome": {
              "version_added": "1"
            },
            "chrome_android": "mirror",
            "edge": {
              "version_added": "12"
            },
            "firefox": {
              "version_added": "1",
              "notes": "Before Firefox 50, border styles of rounded corners (with [`border-radius`](https://developer.mozilla.org/docs/Web/CSS/border-radius)) were always rendered as if `border-top-style` was `solid`. This has been fixed in Firefox 50."
            },
            "firefox_android": "mirror",
            "ie": {
              "version_added": "5.5"
            },
            "oculus": "mirror",
            "opera": {
              "version_added": "9.2"
            },
            "opera_android": "mirror",
            "safari": {
              "version_added": "1"
            },
            "safari_ios": "mirror",
            "samsunginternet_android": "mirror",
            "webview_android": "mirror",
            "webview_ios": "mirror"
          },
          "status": {
            "experimental": false,
            "standard_track": true,
            "deprecated": false
          }
        }
      }
    }
  }
}
