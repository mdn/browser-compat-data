          "support": {
            "chrome": [
              {
                "version_added": "115"
              },
              {
                "version_added": "85",
                "flags": [
                  {
                    "type": "preference",
                    "name": "enable-experimental-web-platform-features",
                    "value_to_set": "enabled"
                  }
                ]
              }
            ],
            "chrome_android": "mirror",
            "edge": "mirror",
