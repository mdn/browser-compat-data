                "flags": [
                  {
                    "type": "preference",
                    "name": "layout.css.scroll-linked-animations.enabled",
                    "value_to_set": "true"
                  }
                ]
              }
            ],
            "firefox_android": "mirror",
            "ie": {
              "version_added": false
            },
            "oculus": "mirror",
            "opera": "mirror",
