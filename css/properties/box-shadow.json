          ],
          "support": {
            "chrome": [
              {
                "version_added": "10",
                "notes": "Shadows affect layout in this browser. For example, if you cast an outer shadow to a box with a `width` of `100%`, then you'll see a scrollbar."
              },
              {
                "prefix": "-webkit-",
                "version_added": "1"
              }
            ],
            "chrome_android": "mirror",
            "edge": {
              "version_added": "12"
