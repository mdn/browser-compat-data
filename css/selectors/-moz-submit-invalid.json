            "opera": "mirror",
            "opera_android": "mirror",
            "safari": {
              "version_added": false
            },
            "safari_ios": "mirror",
            "samsunginternet_android": "mirror",
            "webview_android": "mirror"
          },
          "status": {
            "experimental": false,
            "standard_track": false,
            "deprecated": false
          }
