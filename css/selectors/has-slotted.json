          "tags": [
            "web-features:slot"
          ],
          "support": {
            "chrome": {
              "version_added": false
            },
            "chrome_android": "mirror",
            "edge": "mirror",
            "firefox": {
              "version_added": "preview",
              "flags": [
                {
                  "type": "preference",
                  "name": "layout.css.has-slotted-selector.enabled",
                  "value_to_set": "true"
                }
              ]
            },
            "firefox_android": "mirror",
            "ie": {
              "version_added": false
            },
            "oculus": "mirror",
            "opera": "mirror",
            "opera_android": "mirror",
            "safari": {
              "version_added": false
            },
            "safari_ios": "mirror",
            "samsunginternet_android": "mirror",
            "webview_android": "mirror",
            "webview_ios": "mirror"
          },
          "status": {
            "experimental": true,
            "standard_track": true,
            "deprecated": false
          }
        }
      }
    }
  }
}
