{
  "css": {
    "selectors": {
      "disabled": {
        "__compat": {
          "description": "`:disabled`",
          "mdn_url": "https://developer.mozilla.org/docs/Web/CSS/:disabled",
          "spec_url": [
            "https://html.spec.whatwg.org/multipage/semantics-other.html#selector-disabled",
            "https://drafts.csswg.org/selectors/#enableddisabled"
          ],
          "tags": [
            "web-features:input-selectors"
          ],
          "support": {
            "chrome": {
              "version_added": "1"
            },
            "chrome_android": "mirror",
            "edge": {
              "version_added": "12",
              "notes": "Before Edge 79, Edge did not recognize `:disabled` on the [`&lt;fieldset&gt;`](https://developer.mozilla.org/docs/Web/HTML/Element/fieldset) element."
            },
            "firefox": {
              "version_added": "1"
            },
            "firefox_android": "mirror",
            "ie": {
              "version_added": "9",
              "notes": "Internet Explorer does not recognize `:disabled` on the [`&lt;fieldset&gt;`](https://developer.mozilla.org/docs/Web/HTML/Element/fieldset) element."
            },
            "oculus": "mirror",
            "opera": {
              "version_added": "9"
            },
            "opera_android": {
              "version_added": "10.1"
            },
            "safari": {
              "version_added": "3.1"
            },
            "safari_ios": "mirror",
            "samsunginternet_android": "mirror",
            "webview_android": {
              "version_added": "2"
            },
            "webview_ios": "mirror"
          },
          "status": {
            "experimental": false,
            "standard_track": true,
            "deprecated": false
          }
        }
      }
    }
  }
}
