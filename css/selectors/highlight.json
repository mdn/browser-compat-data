          "tags": [
            "web-features:highlight"
          ],
          "support": {
            "chrome": {
              "version_added": "105"
            },
            "chrome_android": "mirror",
            "edge": "mirror",
            "firefox": {
              "version_added": "preview",
              "impl_url": "https://bugzil.la/1703961",
