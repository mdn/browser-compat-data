{
  "webextensions": {
    "manifest": {
      "action": {
        "__compat": {
          "mdn_url": "https://developer.mozilla.org/docs/Mozilla/Add-ons/WebExtensions/manifest.json/browser_action",
          "support": {
            "chrome": {
              "version_added": "88",
              "notes": [
                "Available for use in Manifest V3 or later.",
                "If an extension defines a browser action, it is not allowed to define a page action as well."
              ]
            },
            "edge": {
              "version_added": "88",
              "notes": "Available for use in Manifest V3 or later."
            },
            "firefox": {
              "version_added": "101",
              "notes": [
                "Available for use in Manifest V3 or later.",
                "Extension developers can turn on Manifest V3 for testing purposes using the <code>extensions.manifestV3.enabled</code> preference."
              ]
            },
            "firefox_android": "mirror",
            "opera": "mirror",
            "safari": {
              "version_added": null,
              "notes": [
                "Available for use in Manifest V3 or later.",
                "If an extension defines a browser action, it is not allowed to define a page action as well."
              ]
            }
          }
        },
        "browser_style": {
          "__compat": {
            "support": {
              "chrome": {
                "version_added": "88"
              },
              "edge": "mirror",
              "firefox": {
                "version_added": "101"
              },
              "firefox_android": {
                "version_added": false
              },
              "opera": "mirror",
              "safari": {
                "version_added": null
              }
            }
          }
        },
        "default_area": {
          "__compat": {
            "support": {
              "chrome": {
                "version_added": "88"
              },
              "edge": "mirror",
              "firefox": {
                "version_added": "101"
              },
              "firefox_android": {
                "version_added": false
              },
              "opera": "mirror",
              "safari": {
                "version_added": null
              }
            }
          }
        },
        "default_icon": {
          "__compat": {
            "support": {
              "chrome": {
                "version_added": "88",
                "notes": "SVG icons are not supported."
              },
              "edge": {
                "version_added": "88",
                "partial_implementation": true,
                "notes": [
                  "SVG icons are not supported.",
                  "'default_icon' must be an object, with explicit sizes."
                ]
              },
              "firefox": {
                "version_added": "101"
              },
              "firefox_android": {
                "version_added": false
              },
              "opera": "mirror",
              "safari": {
                "version_added": null,
                "notes": "SVG icons are not supported. Grayscale images will be treated as template icons and processed with the system accent color and system appearance."
              }
            }
          }
        },
        "default_popup": {
          "__compat": {
            "support": {
              "chrome": {
                "version_added": "88"
              },
              "edge": "mirror",
              "firefox": {
                "version_added": "101"
              },
              "firefox_android": "mirror",
              "opera": "mirror",
              "safari": {
                "version_added": null
              }
            }
          }
        },
        "default_title": {
          "__compat": {
            "support": {
              "chrome": {
                "version_added": "88"
              },
              "edge": "mirror",
              "firefox": {
                "version_added": "101"
              },
              "firefox_android": {
                "version_added": "101",
                "notes": "Browser actions are presented as menu items, and the title is the menu item's label."
              },
              "opera": "mirror",
              "safari": {
                "version_added": null
              }
            }
          }
        },
        "theme_icons": {
          "__compat": {
            "support": {
              "chrome": {
                "version_added": "88"
              },
              "edge": "mirror",
              "firefox": {
                "version_added": "101"
              },
              "firefox_android": {
                "version_added": false
              },
              "opera": "mirror",
              "safari": {
                "version_added": null
              }
            }
          }
        }
      }
    }
  }
}
