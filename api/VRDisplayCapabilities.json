{
  "api": {
    "VRDisplayCapabilities": {
      "__compat": {
        "mdn_url": "https://developer.mozilla.org/docs/Web/API/VRDisplayCapabilities",
        "support": {
          "chrome": {
            "version_added": true,
            "flags": [
              {
                "type": "preference",
                "name": "WebVR"
              }
            ],
            "notes": "Only works on desktop in an <a href='https://webvr.info/get-chrome/'>experimental version of Chrome</a> (other builds won't return any devices when <code>Navigator.getVRDisplays()</code> is invoked)."
          },
          "edge": {
            "version_added": true
          },
          "firefox": [
            {
              "version_added": "55",
              "notes": "Windows support was enabled in Firefox 55."
            },
            {
              "version_added": "58",
              "notes": "macOS support was enabled in Firefox 58."
            }
          ],
          "ie": {
            "version_added": false
          },
          "webview_android": {
            "version_added": false
          },
          "edge_mobile": {
            "version_added": null
          },
          "chrome_android": {
            "version_added": true,
            "notes": "Currently supported only by Google Daydream."
          }
        },
        "status": {
          "experimental": true,
          "standard_track": true,
          "deprecated": false
        }
      },
