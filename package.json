{
  "name": "@mdn/browser-compat-data",
  "version": "4.0.0",
  "description": "Browser compatibility data provided by MDN Web Docs",
  "main": "index.js",
  "types": "index.d.ts",
  "type": "module",
  "engines": {
    "node": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mdn/browser-compat-data.git"
  },
  "keywords": [
    "bcd",
    "browser-compat-data",
    "browser",
    "compatibility",
    "data",
    "mdn",
    "mozilla"
  ],
  "author": "MDN Web Docs",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/mdn/browser-compat-data/issues"
  },
  "homepage": "https://github.com/mdn/browser-compat-data#readme",
  "devDependencies": {
    "ajv": "~6.12.2",
    "better-ajv-errors": "~0.7.0",
    "browser-specs": "~2.7.0",
    "chalk": "~4.1.0",
    "compare-versions": "~3.6.0",
    "mdn-confluence": "~2.2.2",
    "mocha": "~9.1.0",
    "ora": "~5.4.0",
    "prettier": "~2.3.0",
    "yargs": "~17.1.0"
  },
  "scripts": {
    "confluence": "node ./node_modules/mdn-confluence/main/generate.es6.js --output-dir=. --bcd-module=./index.js",
    "mocha": "mocha \"*/**.test.js\"",
    "lint": "node test/lint.js",
    "fix": "node scripts/fix.js",
    "mirror": "node scripts/mirror.js",
    "stats": "node scripts/statistics.js",
    "release-build": "node scripts/release-build.js",
    "release-pulls": "node scripts/release-pulls.js",
    "release-stats": "node scripts/release-stats.js",
    "show-errors": "npm test 1> /dev/null",
    "test": "npm run lint && npm run mocha",
    "traverse": "node scripts/traverse.js"
  }
}
